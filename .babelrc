{
    "presets": [
        "react",
        "es2015",
        "stage-0"
    ],
    "plugins": [
        "dynamic-import-webpack",
        "transform-decorators-legacy",
        [
        "transform-runtime",
        {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime" 
        }
        ]
    ]
    
}